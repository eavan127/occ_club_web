/* ========================================
   FRAMEWORK SECTION (About & Mission Redesign)
   ======================================== */
.framework-section {
  padding: 120px 5%;
  background-color: #000;
  color: #fff;
  position: relative;
  overflow: hidden;
}

.framework-container {
  max-width: 1400px;
  margin: 0 auto;
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 80px;
  align-items: start;
}

/* Left Column */
.framework-left {
  position: sticky;
  top: 100px;
  /* Sticky scroll effect */
}

.framework-label {
  font-family: var(--font-display);
  font-size: 0.9rem;
  letter-spacing: 2px;
  color: #666;
  text-transform: uppercase;
  margin-bottom: 30px;
  display: block;
}

.framework-headline {
  font-family: var(--font-display);
  font-size: clamp(2.5rem, 5vw, 4.5rem);
  line-height: 1.1;
  font-weight: 500;
  margin-bottom: 40px;
  background: linear-gradient(to right, #fff, #aaa);
  -webkit-background-clip: text;
  background-clip: text;
  -webkit-text-fill-color: transparent;
}

.framework-description {
  font-family: var(--font-body);
  font-size: 1.1rem;
  line-height: 1.8;
  color: #a0a0a0;
  margin-bottom: 50px;
  max-width: 500px;
}

.btn-framework {
  padding: 16px 40px;
  background: #2F55D4;
  color: #fff;
  border: none;
  font-family: var(--font-display);
  font-size: 0.9rem;
  font-weight: 700;
  letter-spacing: 1px;
  text-transform: uppercase;
  cursor: pointer;
  transition: all 0.3s ease;
  display: inline-flex;
  align-items: center;
  gap: 10px;
}

.btn-framework:hover {
  background: white;
  color: #000;
  transform: translateY(-2px);
}

.framework-left {
  position: sticky;
  top: 150px;
  height: fit-content;
}

/* Right Column: Timeline */
.framework-timeline {
  position: relative;
  border-left: 1px solid rgba(255, 255, 255, 0.1);
  padding-left: 50px;
}

.timeline-item {
  position: relative;
  padding-bottom: 40vh;
  opacity: 0.5;
  /* Default dim */
  transition: opacity 0.5s ease, transform 0.5s ease;
}

.timeline-item:hover,
.timeline-item.active {
  opacity: 1;
}

.timeline-marker {
  position: absolute;
  left: -70px;
  /* Adjust based on padding */
  width: 40px;
  height: 40px;
  border: 1px solid #2F55D4;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-family: var(--font-display);
  font-size: 0.9rem;
  color: #2F55D4;
  background: #000;
  z-index: 2;
  transition: all 0.3s ease;
}

.timeline-item:hover .timeline-marker,
.timeline-item.active .timeline-marker {
  background: #2F55D4;
  color: #fff;
  box-shadow: 0 0 20px rgba(47, 85, 212, 0.4);
}

/* 3D Rotation Animation */
@keyframes rotate3dFlow {
  0% {
    transform: perspective(1000px) rotateY(-12deg) rotateX(5deg) scale(0.95);
  }

  50% {
    transform: perspective(1000px) rotateY(12deg) rotateX(-5deg) scale(0.95);
  }

  100% {
    transform: perspective(1000px) rotateY(-12deg) rotateX(5deg) scale(0.95);
  }
}

.timeline-visual {
  width: 100%;
  height: 250px;
  object-fit: cover;
  border-radius: 12px;
  margin-top: 30px;
  border: 1px solid rgba(168, 240, 255, 0.2);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.4);
  opacity: 0.9;
  transition: all 0.5s ease;
  animation: rotate3dFlow 6s ease-in-out infinite alternate;
  will-change: transform;
}

.timeline-visual:hover {
  opacity: 1;
  animation-play-state: paused;
  transform: perspective(1000px) rotateY(0deg) rotateX(0deg) scale(1.05);
  border-color: #2F55D4;
  box-shadow: 0 0 30px rgba(47, 85, 212, 0.4);
}

.timeline-title {
  font-family: var(--font-display);
  font-size: 2rem;
  font-weight: 500;
  color: #fff;
  margin-bottom: 15px;
  text-transform: uppercase;
}

.timeline-desc {
  font-family: var(--font-body);
  font-size: 1rem;
  color: #888;
  line-height: 1.6;
}

/* Page Transition Overlay */
.page-transition-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background: #000;
  z-index: 9999;
  transform: scaleY(0);
  /* Hidden by default */
  transform-origin: bottom;
  pointer-events: none;
  transition: transform 0.6s cubic-bezier(0.7, 0, 0.3, 1);
}

.page-transition-overlay.active {
  transform: scaleY(1);
  transform-origin: bottom;
  pointer-events: all;
}

.page-transition-overlay.exit {
  transform: scaleY(0);
  transform-origin: top;
}

/* Mobile Responsive */
@media (max-width: 992px) {
  .framework-container {
    grid-template-columns: 1fr;
    gap: 60px;
  }

  .framework-left {
    position: relative;
    top: 0;
  }

  .framework-timeline {
    padding-left: 30px;
  }

  .timeline-marker {
    left: -50px;
  }
}

/* ========================================
   DRIVE SECTION (White Background)
   ======================================== */
.drive-section {
  padding: 120px 5%;
  background-color: #fff;
  color: #111;
  position: relative;
}

.drive-header {
  text-align: center;
  max-width: 900px;
  margin: 0 auto 80px;
}

.drive-headline {
  font-family: var(--font-display);
  font-size: clamp(2.5rem, 5vw, 4rem);
  font-weight: 500;
  margin-bottom: 25px;
  letter-spacing: -1px;
}

.drive-subtext {
  font-family: var(--font-body);
  font-size: 1.1rem;
  color: #666;
  line-height: 1.6;
}

.drive-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 30px;
  max-width: 1400px;
  margin: 0 auto;
}

.drive-grid-bottom {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 30px;
  max-width: 1400px;
  /* Full width matches top grid */
  margin: 30px auto 0;
}

.drive-card {
  background: #fff;
  padding: 40px;
  border: 1px solid #e5e5e5;
  /* Subtle border */
  border-radius: 0;
  position: relative;
  transition: transform 0.3s ease, border-color 0.3s ease;
  height: 100%;
}

.drive-card:hover {
  transform: translateY(-2px);
  border-color: #111;
  box-shadow: 0 10px 30px rgba(0, 0, 0, 0.05);
}

.card-header {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 30px;
}

.card-title {
  font-family: var(--font-display);
  font-size: 1.8rem;
  font-weight: 400;
  margin: 0;
  letter-spacing: -0.5px;
}

.card-icon-box {
  width: 48px;
  height: 48px;
  background: #111;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 1.4rem;
}

.card-desc {
  font-family: var(--font-body);
  font-size: 1rem;
  color: #555;
  line-height: 1.6;
}

/* Responsive Drive Grid */
@media (max-width: 992px) {
  .drive-grid {
    grid-template-columns: repeat(2, 1fr);
  }
}

@media (max-width: 768px) {

  .drive-grid,
  .drive-grid-bottom {
    grid-template-columns: 1fr;
    max-width: 100%;
    gap: 20px;
    margin-top: 20px;
  }
}