/* ========================================
   HERO SECTION - MODERN REDESIGN
   ======================================== */
#banner_data {
  position: relative;
  text-align: center;
  max-width: 1200px;
  padding: 0 20px;
}

/* Main Hero Title */
.hero-title {
  font-family: var(--font-display);
  font-size: clamp(3rem, 10vw, 8rem);
  font-weight: 900;
  line-height: 1.05;
  letter-spacing: -0.03em;
  margin-bottom: var(--space-md);
  color: #ffffff;
}

.hero-title .hero-accent {
  display: block;
  background: var(--accent-gradient);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

/* Animated Rotating Words */
.hero-rotate-container {
  display: inline-block;
  position: relative;
  min-width: 300px;
  height: 1.2em;
  overflow: hidden;
  vertical-align: bottom;
}

.hero-rotate-word {
  display: block;
  position: absolute;
  width: 100%;
  left: 0;
  opacity: 0;
  transform: translateY(100%);
  background: var(--accent-gradient);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  animation: rotateWord 12s infinite;
}

.hero-rotate-word:nth-child(1) {
  animation-delay: 0s;
}

.hero-rotate-word:nth-child(2) {
  animation-delay: 3s;
}

.hero-rotate-word:nth-child(3) {
  animation-delay: 6s;
}

.hero-rotate-word:nth-child(4) {
  animation-delay: 9s;
}

@keyframes rotateWord {

  0%,
  2% {
    opacity: 0;
    transform: translateY(100%);
  }

  5%,
  20% {
    opacity: 1;
    transform: translateY(0);
  }

  23%,
  100% {
    opacity: 0;
    transform: translateY(-100%);
  }
}

/* Hero Subtitle */
.hero-subtitle {
  font-family: var(--font-body);
  font-size: clamp(1rem, 2.5vw, 1.5rem);
  font-weight: 400;
  color: rgba(255, 255, 255, 0.7);
  margin-bottom: var(--space-lg);
  letter-spacing: 0.05em;
}

/* CTA Buttons Container */
.hero-cta {
  display: flex;
  gap: var(--space-md);
  justify-content: center;
  flex-wrap: wrap;
  margin-top: var(--space-md);
}

/* Primary Button - Filled */
.btn-primary {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 16px 40px;
  font-family: var(--font-body);
  font-size: 0.95rem;
  font-weight: 600;
  text-decoration: none;
  text-transform: uppercase;
  letter-spacing: 0.1em;
  color: var(--dark-bg);
  background: var(--primary);
  border: 2px solid var(--primary);
  border-radius: 50px;
  cursor: pointer;
  position: relative;
  overflow: hidden;
  transition: all var(--transition-normal);
  box-shadow: 0 4px 20px rgba(168, 240, 255, 0.3);
}

.btn-primary:hover {
  transform: translateY(-3px);
  box-shadow: var(--shadow-glow-strong);
  background: var(--primary-light);
}

.btn-primary:active {
  transform: translateY(-1px);
}

/* Secondary Button - Outlined/Glass */
.btn-secondary {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 16px 40px;
  font-family: var(--font-body);
  font-size: 0.95rem;
  font-weight: 600;
  text-decoration: none;
  text-transform: uppercase;
  letter-spacing: 0.1em;
  color: var(--primary);
  background: var(--glass-bg);
  border: 2px solid var(--glass-border);
  border-radius: 50px;
  cursor: pointer;
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  transition: all var(--transition-normal);
}

.btn-secondary:hover {
  background: var(--glass-bg-hover);
  border-color: var(--glass-border-hover);
  transform: translateY(-3px);
  box-shadow: var(--shadow-glow);
}

/* Legacy hero styles - kept for compatibility */
#hackclub {
  font-family: var(--font-display);
  font-size: clamp(3rem, 10vw, 8rem);
  font-weight: 900;
  color: var(--primary);
  line-height: 1.1;
  letter-spacing: -0.03em;
  margin-bottom: 10px;
  text-shadow: 0 0 60px rgba(168, 240, 255, 0.3);
}

#hackclub-des {
  font-family: var(--font-body);
  font-size: clamp(1rem, 3vw, 1.8rem);
  font-weight: 400;
  color: rgba(255, 255, 255, 0.7);
  letter-spacing: 0.08em;
  text-transform: uppercase;
}

/*scrollbar changes*/
.no_scroll_bar_visible::-webkit-scrollbar {
  display: none;
}

.scroll_bar_visible::selection {
  background-color: grey;
}

.scroll_bar_visible::-webkit-scrollbar {
  width: 10px !important;
  background-color: darkslategrey;
}

.scroll_bar_visible::-webkit-scrollbar-thumb {
  background-color: #A8F0FF;
}

/* Full-screen overlay */
#loader_fixed_container {
  position: fixed;
  /* <— pin to viewport */
  inset: 0;
  /* top/right/bottom/left: 0 */
  background: #0A0A0A;
  /* solid bg so content doesn't show through */
  display: grid;
  place-items: center;
  z-index: 9999;
  /* <— above everything */
  opacity: 1;
  /* start visible */
  transition: opacity .3s ease;
  /* smooth fade */
}

/*lock scroll while loader is visible*/
.no_scroll_bar_visible {
  overflow: hidden;
}

/*restore scroll after loader fades out */
.scroll_bar_visible {
  overflow: auto;
}

/* ========================================
   ABOUT SECTION - MODERN REDESIGN
   ======================================== */
